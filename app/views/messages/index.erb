<div class="header">
  <div class="container">

    <h1>Messenger</h1>
  </div>
</div>

<div class="messages">
  <div class="container">

    <!-- Your code here -->
    <% @messages.each do |message| %>
        <div class="message">
          <p class="content"><%= message.content %></p>
          <p class="time"><%= message.created_at %></p>
        </div>
    <% end %>

    <%= link_to 'New Message' , "messages/new" %>

  </div>
</div>
